<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Day29" parent="Theme.Material3.DayNight">
        <!-- Primary brand color. -->

        <item name="colorPrimary">#616200</item>
        <item name="colorPrimaryContainer">#e7e970</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>

        <item name="colorOnSecondaryContainer">#48482d</item>
        <item name="colorSecondaryContainer">#48482d</item>
        <!-- Status bar color. -->

    </style>
<style name="one" parent="Theme.Material3.Light.NoActionBar">

    <item name="colorPrimary">#616200</item>
    <item name="colorPrimaryContainer">#e7e970</item>
    <item name="colorPrimaryVariant">@color/purple_700</item>
    <item name="colorOnPrimary">@color/white</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">@color/teal_200</item>
